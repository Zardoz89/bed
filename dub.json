{
  "name": "bed",
  "description": "Small BDD testing framework for D.",
  "copyright": "Copyright Â© 2014, Pedro Tacla Yamada",
  "license": "MIT",
  "authors": [
    "Pedro Tacla Yamada"
  ],
  "dependencies": {
    "colorize": ">=1.0.1"
  },
  "configurations": [
    {
      "name": "bed",
      "sourcePaths": [
        "source"
      ]
    },
    {
      "name": "unittest",
      "targetType": "executable",
      "sourcePaths": [
        "source",
        "tests"
      ],
      "buildOptions": [
        "unittests"
      ],
      "dflags": [
        "-main"
      ]
    },
    {
      "name": "examples",
      "targetType": "executable",
      "sourcePaths": [
        "examples"
      ],
      "buildOptions": [
        "unittests"
      ],
      "dflags": [
        "-main"
      ]
    }
  ]
}
